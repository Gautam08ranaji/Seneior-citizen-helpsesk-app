{
  "expo": {
    "name": "Seneior Citizen helpdesk App",
    "slug": "seneior-citizen-helpdesk-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appIcon1.png",
    "scheme": "seneiorcitizenhelpdeskapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appIcon1.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCs-N8XxSQWIfllyifIcpNLKANPTWnd2jk"
        }
      },
      "package": "com.gautamranaji.seneiorcitizenhelpdeskapp"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to show the route on map.",
        "NSLocationAlwaysUsageDescription": "We need your location to show the route even when app is in background."
      },
      "bundleIdentifier": "com.gautamranaji.seneiorcitizenhelpdeskapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/appIcon1.png",
          "resizeMode": "contain",
          "backgroundColor": "#D8ECFF"
        }
      ],
      "expo-localization",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b8057c07-23d1-4321-945e-1376e7872686"
      }
    },
    "owner": "gautam1008"
  }
}

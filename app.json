{
  "expo": {
    "name": "FieldTrackApp",
    "slug": "FieldTrackingApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appIcon1.png",
    "scheme": "fieldtrackingapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appIcon1.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCs-N8XxSQWIfllyifIcpNLKANPTWnd2jk"
        }
      },
      "package": "com.anonymous.FieldTrackingApp"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to show the route on map.",
        "NSLocationAlwaysUsageDescription": "We need your location to show the route even when app is in background."
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/appIcon1.png",
          "resizeMode": "contain",
          "backgroundColor": "#D8ECFF"
        }
      ],
      "expo-localization",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8293a5a0-5ca9-4e32-85ac-2e767af8c0a0"
      }
    }
  }
}
